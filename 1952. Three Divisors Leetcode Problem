class Solution {
    public boolean isThree(int n) {
          if(n==2){
              return false;
          }
           if(n==4){
              return true;
          }
        int gcd=1;
      for(int i=2;i<=n/2;i++){
          if(n%i==0)
          gcd++;
      }
          if(gcd==2){
              return true;
          }else{
              return false;
          }  
    }
} 
